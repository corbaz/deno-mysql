#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/mnt/c/www/0-denoApiMysql/node_modules/.pnpm/gh-pages@3.1.0/node_modules/gh-pages/bin/gh-pages-clean.js/node_modules:/mnt/c/www/0-denoApiMysql/node_modules/.pnpm/gh-pages@3.1.0/node_modules/gh-pages/bin/node_modules:/mnt/c/www/0-denoApiMysql/node_modules/.pnpm/gh-pages@3.1.0/node_modules/gh-pages/node_modules:/mnt/c/www/0-denoApiMysql/node_modules/.pnpm/gh-pages@3.1.0/node_modules:/mnt/c/www/0-denoApiMysql/node_modules/.pnpm/node_modules:/mnt/c/www/0-denoApiMysql/node_modules:/mnt/c/www/node_modules:/mnt/c/node_modules:/mnt/c/www/0-denoApiMysql/node_modules/gh-pages/bin/gh-pages-clean.js/node_modules:/mnt/c/www/0-denoApiMysql/node_modules/gh-pages/bin/node_modules:/mnt/c/www/0-denoApiMysql/node_modules/gh-pages/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../gh-pages/bin/gh-pages-clean.js" "$@"
else 
  exec node  "$basedir/../gh-pages/bin/gh-pages-clean.js" "$@"
fi
